import java.util.Random;

public class Main {
  public static void main(String[] args) {
    System.out.println("생성된 전화번호 :");

    Random rand = new Random();

    int two = rand.nextInt(5) + 2;
    int three = rand.nextInt(7) + 1;
    int z = rand.nextInt(10000000);

    String str2 = String.valueOf(z);

    String str = "0" + two + three + " - " + str2.substring(0,3) + " - " + str2.substring(3);

    System.out.println(str);
  }
}
