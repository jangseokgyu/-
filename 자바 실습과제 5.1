import java.util.*;

public class Main {
  public static void main(String[] args) {
    int i=0,f=0,p=0;
    int point=0;
    Scanner scan = new Scanner(System.in);

    while(true) {
      if(i==0) 
        System.out.println("첫 점수를 입력하세요.");

      else
        System.out.println("다음 점수를 입력하세요 : ");

      point = scan.nextInt();

      if(point >=60)
        p++;
      else
        f++;

      i++;

      if(point < 0) {
        i--;
        f--;
        break;
       }
    }

  System.out.println("학생수 : "+ i + "\n" + "통과 : " + p + "\n" + "낙제 : " + f);
}
}
